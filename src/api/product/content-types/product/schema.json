{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "products",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 50
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "rating": {
      "type": "json"
    },
    "images": {
      "type": "text"
    },
    "productUID": {
      "type": "uid",
      "targetField": "title"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    }
  }
}
