{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "categories",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 30
    },
    "description": {
      "type": "string"
    },
    "nameuid": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "categoryuid": {
      "type": "customField",
      "required": true,
      "customField": "plugin::field-uuid.uuid"
    },
    "subcategories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subcategory.subcategory",
      "mappedBy": "category"
    }
  }
}
